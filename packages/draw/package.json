{
  "name": "@mindgraph/draw",
  "version": "0.0.1",
  "description": "markdown note graph view builder",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint src/**",
    "typecheck": "tsc --noEmit"
  },
  "private": true,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/mindgraph.umd.cjs",
  "module": "./dist/mindgraph.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mindgraph.js",
      "require": "./dist/mindgraph.umd.cjs"
    }
  },
  "devDependencies": {
    "@types/d3-array": "^3.0.4",
    "@types/d3-force": "^3.0.4",
    "@types/d3-selection": "^3.0.4",
    "@types/d3-zoom": "^3.0.2",
    "koa": "^2.14.1",
    "vite": "^4.0.0",
    "vitest": "^0.27.0",
    "@mindgraph/types": "workspace:*",
    "@mindgraph/read": "workspace:*"
  },
  "dependencies": {
    "d3-array": "^3.2.1",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0"
  }
}
